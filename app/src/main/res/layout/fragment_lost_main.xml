<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayLost_Main"
    android:background="@color/primary_dark"
    tools:context="com.team3s.lostpropertyse.MainPage.FindMainFrag">
    <android.support.design.widget.AppBarLayout
        android:id="@+id/LostappBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include
            android:id="@+id/toolbar"
            layout="@layout/lost_feed_toolbar" />
    </android.support.design.widget.AppBarLayout>

    <FrameLayout
        android:id="@+id/lostmainfrag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/LostappBarLayout"
        android:background="@color/primary_dark"
        tools:ignore="RtlCompat">

        <ListView
            android:id="@+id/list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:stackFromBottom="true"
            android:transcriptMode="normal"/>

    </FrameLayout>

    <LinearLayout
        android:id="@+id/postdetpfr"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/LostappBarLayout"
        android:orientation="vertical"
        tools:ignore="RtlCompat" />
</RelativeLayout>
